<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:james="https://jamesnet.dev/xaml/presentation"
    xmlns:units="clr-namespace:BlurSharp.Main.UI.Units"
    xmlns:views="clr-namespace:BlurSharp.Main.UI.Views">
    <Style TargetType="{x:Type views:MainContent}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type views:MainContent}">
                    <james:JamesGrid Columns="*" Rows="*">
                        <!--<units:Tab Background="#252525" ItemsSource="{Binding Tabs}" />-->
                        <james:JamesRegion Background="#2d2f3a" RegionName="TabRegion" />
                    </james:JamesGrid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>