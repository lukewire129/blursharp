<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:jaems="clr-namespace:Jamesnet.Wpf.Controls;assembly=Jamesnet.Wpf"
    xmlns:units="clr-namespace:BlurSharp.Project.UI.Units">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="/BlurSharp.Project;component/Themes/Units/CommonStyle.xaml" />
    </ResourceDictionary.MergedDictionaries>
    <Style TargetType="{x:Type units:SeedTextBox}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type units:SeedTextBox}">
                    <jaems:JamesGrid Columns="auto, auto, auto" Rows="*">
                        <Label Content="Seed :" Foreground="White" />
                        <units:RoundTextBox Width="500" />
                        <CheckBox
                            VerticalAlignment="Center"
                            Content="Generate Debug Symbols"
                            Foreground="White" />
                    </jaems:JamesGrid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>