<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:units="clr-namespace:BlurSharp.Project.UI.Units"
                    xmlns:jaems="clr-namespace:Jamesnet.Wpf.Controls;assembly=Jamesnet.Wpf">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="/BlurSharp.Project;component/Themes/Units/CommonStyle.xaml"/>
    </ResourceDictionary.MergedDictionaries>
    <Style TargetType="{x:Type units:SedTextBox}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type units:SedTextBox}">
                    <jaems:JamesGrid Columns="auto, auto, auto" Rows="*">
                        <Label Content="Sed :" Style="{StaticResource TitleLabel}"/>
                        <units:RoundTextBox Width="600"/>
                        <CheckBox VerticalAlignment="Center" Content="Generate Debug Symbols" Foreground="White"/>
                    </jaems:JamesGrid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>