<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:views="clr-namespace:BlurSharp.Forms.UI.Views"
                    xmlns:based="clr-namespace:DarkSkinWindow.UI.Views;assembly=DarkSkinWindow"
                    xmlns:james="clr-namespace:Jamesnet.Wpf.Controls;assembly=Jamesnet.Wpf">
    <Style TargetType="{x:Type views:BlurSharpWindow}"
           BasedOn="{StaticResource {x:Type based:DarkWindow}}">
        <Setter Property="Content">
            <Setter.Value>
                <james:JamesRegion RegionName="MainRegion"/>
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>